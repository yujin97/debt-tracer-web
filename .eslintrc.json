{
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "airbnb/hooks",
    "next/core-web-vitals",
    "prettier"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/quotes": "off",
    "react/jsx-one-expression-per-line": "off",
    "import/prefer-default-export": "off",
    "no-use-before-define": [
      "error",
      {
        "variables": false,
        "functions": false
      }
    ]
  }
}
